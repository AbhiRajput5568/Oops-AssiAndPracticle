#include <iostream>
using namespace std;

class MyClass
{
public:
    void outerFunction()
    {
        cout << "In outerFunction, calling innerFunction..." << endl;
        innerFunction();
    }

private:
    void innerFunction()
    {
        cout << "This is innerFunction, called from outerFunction." << endl;
    }
};

int main()
{
    cout << "Abhishek Singh (2315272)\n";

    MyClass obj;
    obj.outerFunction();
    return 0;
}
